                    <StackLayout Orientation="Vertical">
                -->
                    <Grid RowDefinitions="<#= rowDefinitionsRowDefinitionsString #>" ColumnDefinitions="*">    
<#

        bool isNewLine = false;
        bool gridOpened = false;
