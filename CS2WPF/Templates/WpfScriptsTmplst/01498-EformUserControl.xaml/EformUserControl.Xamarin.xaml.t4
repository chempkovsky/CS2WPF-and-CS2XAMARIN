             xmlns:invboolconv="<#= invboolconvPrefix #>"
             xmlns:str2boolconv="<#= strnulltoboolPrefix #>"
             xmlns:nullableback="<#= nullablebackPrefix #>"
             

<# 
    if (ModelHasAutoCompleteTextBox) {
#>
