        }
        
        rowDefinitionsRowDefinitionsString = rowDefinitionsRowDefinitionsString + string.Concat(Enumerable.Repeat(", Auto", gridRowCount+1));
    }
    for (int InputTypeId = 1; InputTypeId <= 3; InputTypeId++) {
